      section.page-block
        h2.text-center.font-family-press-start.font-medium.margin-bottom-large CLI

        div.table-wrapper
          table
            thead
              tr
                th Option
                th Description
            tbody
              tr
                td -c, --config
                td Path to config file
              tr
                td -v, --verbose
                td Output all test data (default: false)
              tr
                td -V, --version
                td Output the version number
              tr
                td -h, --help
                td Output usage information
      
      hr
      
      section.page-block
        h2.text-center.font-family-press-start.font-medium.margin-bottom-large Config

        div.table-wrapper
          table
            thead
              tr
                th Attribute
                th Type
                th Description
            tbody
              tr
                td urls
                td Array[String]
                td An array of urls that you would like to run the tests against
              tr
                td tests
                td Array[Object]
                td An array of test objects
              tr
                td tests.viewports.width
                td Object/Number (optional)
                td The viewport width (in pixels) to test against. Defaults to 800
              tr
                td tests.viewports.height
                td Object/Number (optional)
                td The viewport height (in pixels) to test against. Defaults to 600
              tr
                td tests.expectedPropertyValues
                td Object
                td A map of css properies and allowed values (or tokens)</td>
              
        h3.font-medium.margin-top-large.margin-bottom-large tests.expectedPropertyValues[Object]

        div.table-wrapper
          table
            thead
              tr
                th Attribute
                th Type
                th Suppoorted values / units
                
            tbody
              each property in properties
                tr
                  td #{property.name}
                  td #{property.type}
                  - var supports = property.supported.join(', ')
                  if property.supportedInfo
                    td  #{supports} - #{property.supportedInfo}
                  else
                    td  #{supports}

